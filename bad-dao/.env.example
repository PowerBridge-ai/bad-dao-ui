# Network Private Key (without 0x prefix)
PRIVATE_KEY=your_wallet_private_key_here

# Block Explorer API Key for contract verification
# For Base, you can use Etherscan API key
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# RPC URLs (optional - defaults are in hardhat.config.js)
BASE_MAINNET_URL=https://mainnet.base.org
BASE_GOERLI_URL=https://goerli.base.org

# Gas settings (optional)
GAS_PRICE=1000000000
GAS_LIMIT=8000000

# Deployment accounts (optional - comma-separated addresses)
# If specified, these will be additional owners besides the deployer
MULTISIG_OWNERS=
TREASURY_ADMINS=
TREASURY_EXECUTORS= 